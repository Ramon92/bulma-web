{
  "name": "@bulma-web/media",
  "version": "0.0.1",
  "description": "The media object useful for social media interfaces, but useful in any context.",
  "keywords": [
    "web-component",
    "media",
    "bulma-web",
    "bw-media",
    "lit-element",
    "lit-html"
  ],
  "author": "bulma-web",
  "homepage": "https://bulma-web.netlify.com",
  "license": "MIT",
  "main": "bw-media.js",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ramon92/bulma-web.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "yarn build:scss && yarn build:js && yarn build:types",
    "build:js": "babel src -d ./ --extensions \".ts\" --config-file ./.babelrc --source-maps inline",
    "build:storybook": "yarn build:scss && yarn build:storybook:src && yarn build:storybook:stories",
    "build:storybook:src": "babel src -d src --extensions \".ts\" --config-file ./.babelrc --source-maps inline",
    "build:storybook:stories": "babel stories -d stories --extensions \".ts\" --config-file ./.babelrc --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:scss": "node-sass src -o src"
  },
  "bugs": {
    "url": "https://github.com/Ramon92/bulma-web/issues"
  },
  "dependencies": {
    "@bulma-web/content": "^0.0.4",
    "lit-element": "^2.2.1"
  }
}
